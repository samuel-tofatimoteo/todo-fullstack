{
  "name": "react-query-intro",
  "version": "0.2.4",
  "description": "An introduction to React Query",
  "main": "server/index.ts",
  "author": "Dev Academy Aotearoa",
  "license": "ISC",
  "scripts": {
    "start": "node dist/server.js",
    "test": "vitest",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "dist"
    ]
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.17.2",
    "knex": "^2.3.0",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.9.1",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.14",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/react-router": "^5.1.19",
    "@types/superagent": "^4.1.16",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-c8": "^0.31.0",
    "esbuild": "^0.19.1",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "nock": "^13.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "superagent": "7.1.1",
    "supertest": "^6.2.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}